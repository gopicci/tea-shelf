{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(ts|tsx)$"
  },
  "sourceType": "script",
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/component",
    "node_modules/better-docs/category"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "better-docs": {
      "name": "Tea Shelf UI Documentation",
      "logo": "https://teashelf.app/logo192.png",
      "hideGenerator": true,
      "css": "styles/jsdoc.css",
      "navLinks": [
        {
              "label": "API Documentation",
              "href": "https://teashelf.app/docs/api/"
          },
          {
              "label": "Github",
              "href": "https://github.com/gopicci/tea-shelf"
          }
      ]
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  }
}
